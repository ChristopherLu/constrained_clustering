function [ dist ] = multDifference( x,y )
 x
 y
 
dist = 0

for i = 1:1:length(x)
   num = x(i);
   den = y(i);
   frac = num / den;
   if y(i) == 0
       if x(i) ~= 0
           dist = inf;
           break;
       else
           frac = 1;
       end
   end
    
    dist = dist + x(i)*log(x(i) / y(i))
end

end

